{
  "name": "fs-match",
  "version": "1.3.1",
  "description": "A NodeJS package for finding folders, files & apps by Regular expression based on Asynchronous iteration of ES 9",
  "main": "dist/core.js",
  "bin": {
    "fs-match": "dist/find.js",
    "app-find": "dist/which.js"
  },
  "engines": {
    "node": "^5.12.0"
  },
  "scripts": {
    "build": "node dist/compile source/ dist/  &&  esdoc",
    "prepublishOnly": "npm run build",
    "start": "web-server docs/ -o",
    "lint": "eslint source/",
    "check": "mocha -r babel-register -r babel-polyfill -r should --no-timeouts -c --inline-diffs",
    "test": "npm run check -- --exit",
    "debug": "npm run check -- --inspect-brk",
    "precommit": "npm run lint  &&  npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechQuery/fs-match.git"
  },
  "keywords": [
    "search",
    "filesystem",
    "regex-match",
    "command-line",
    "async",
    "iterator"
  ],
  "author": "shiy007@qq.com",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/TechQuery/fs-match/issues"
  },
  "homepage": "https://techquery.github.io/fs-match/",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "koapache": "^0.2.3",
    "mocha": "^5.2.0",
    "should": "^13.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "commander": "^2.15.1",
    "fs-extra": "^6.0.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-async-generator-functions",
      "transform-runtime"
    ]
  }
}
